{
  "name": "price-sync-dashboard",
  "version": "1.0.0",
  "description": "Price synchronization dashboard for StreetPricer, WooCommerce, and Shopify",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "dev": "bun --watch src/index.ts",
    "dev:all": "./dev.sh",
    "test": "bun test",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "price-sync",
    "woocommerce",
    "shopify",
    "streetpricer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "supertest": "^7.1.4"
  }
}